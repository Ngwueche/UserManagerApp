<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Dashboard</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/UserManagerApp.styles.css" asp-append-version="true" />
</head>
<body>
    
    <partial name="_NavBArPartial" />

    <div class="drawer-panel">
        <div class="side-drawer">
            <p>Dashboard</p>
            <p>Manage Users</p>
            <p>Manage Roles</p>
        </div>
        <div class="drawer-content-area">
            <div class="drawer-content-area-inner">
                lorem50 fsgyn wewtn etweb wetrhwqwr wqvqrq qr35hbv wqfgh th2gqvcw
                @RenderBody()
            </div>
        </div>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - UserManagerApp - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
